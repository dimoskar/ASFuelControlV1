              </xs:annotation>
            </xs:element>
            <xs:element name="F_REASONING" type="xs:string">
              <xs:annotation>
                <xs:documentation>Περιγραφή σκοπού επέμβασης.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="F_HM_ENARXHS" type="xs:date">
              <xs:annotation>
                <xs:documentation>Ημερομηνία / Ώρα έναρξης εργασιών</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="F_HM_LIXIS" type="xs:date">
              <xs:annotation>
                <xs:documentation>Ημερομηνία / Ώρα λήξης εργασιών</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="F_COMMEMNTS" type="xs:string">
